{
  "name": "sync-video-engine-client",
  "version": "1.0.0",
  "description": "Desktop client for video synchronization",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "dist": "electron-builder --mac"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.vsengine.syncclient",
    "productName": "Video Sync Engine",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.video"
    }
  }
}